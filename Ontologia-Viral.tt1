<http://www.semanticweb.org/ontologia-viral> rdf:type owl:Ontology .

#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/ontologia-viral#TemDNA
:TemDNA rdf:type owl:ObjectProperty ,
                 owl:FunctionalProperty ,
                 owl:ReflexiveProperty ;
        rdfs:domain :Grupo_I ,
                    :Grupo_II ,
                    :Grupo_VII ;
        rdfs:comment <http://www.semanticweb.org/Virus de DNA> .


###  http://www.semanticweb.org/ontologia-viral#TemDuplaFita
:TemDuplaFita rdf:type owl:ObjectProperty ,
                       owl:FunctionalProperty ,
                       owl:ReflexiveProperty ;
              rdfs:domain :Grupo_I ,
                          :Grupo_III ,
                          :Grupo_VII .


###  http://www.semanticweb.org/ontologia-viral#TemFitaSimples
:TemFitaSimples rdf:type owl:ObjectProperty ;
                rdfs:domain :Grupo_II ,
                            :Grupo_IV ,
                            :Grupo_V ,
                            :Grupo_VI .


###  http://www.semanticweb.org/ontologia-viral#TemRNA
:TemRNA rdf:type owl:ObjectProperty ;
        rdfs:domain :Grupo_III ,
                    :Grupo_IV ,
                    :Grupo_V ,
                    :Grupo_VI ;
        rdfs:range :Grupo_III ,
                   :Grupo_IV ,
                   :Grupo_V ,
                   :Grupo_VI ;
        rdfs:comment <http://www.semanticweb.org/Virus de RNA> .


###  http://www.semanticweb.org/ontologia-viral#TemSensoNegativo
:TemSensoNegativo rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :TemFitaSimples ;
                  owl:inverseOf :TemSensoPositivo ;
                  rdf:type owl:FunctionalProperty ;
                  rdfs:domain :Grupo_IV ;
                  rdfs:range :Grupo_IV ;
                  owl:propertyDisjointWith :TemSensoPositivo .


###  http://www.semanticweb.org/ontologia-viral#TemSensoPositivo
:TemSensoPositivo rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :TemFitaSimples ;
                  rdf:type owl:FunctionalProperty ;
                  rdfs:domain :Grupo_V ,
                              :Grupo_VI ;
                  rdfs:range :Grupo_V ,
                             :Grupo_VI .


###  http://www.semanticweb.org/ontologia-viral#TemTransiçãoReversa
:TemTransiçãoReversa rdf:type owl:ObjectProperty ,
                              owl:FunctionalProperty .


###  http://www.semanticweb.org/ontologia-viral#eCircular
:eCircular rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/ontologia-viral#eLinear
:eLinear rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/ontologia-viral#eSegmentada
:eSegmentada rdf:type owl:ObjectProperty .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/ontologia-viral#Especies
:Especies rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf owl:topDataProperty ;
          rdfs:range xsd:string .


###  http://www.semanticweb.org/ontologia-viral#Familia
:Familia rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf owl:topDataProperty ;
         rdfs:range xsd:string .


###  http://www.semanticweb.org/ontologia-viral#Generos
:Generos rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf owl:topDataProperty ;
         rdfs:range xsd:string .


###  http://www.semanticweb.org/ontologia-viral#Grupo
:Grupo rdf:type owl:DatatypeProperty ;
       rdfs:subPropertyOf owl:topDataProperty ;
       rdfs:domain :Grupo_I ,
                   :Grupo_II ,
                   :Grupo_III ,
                   :Grupo_IV ,
                   :Grupo_V ,
                   :Grupo_VI ,
                   :Grupo_VII .


###  http://www.semanticweb.org/ontologia-viral#Ordem
:Ordem rdf:type owl:DatatypeProperty .


###  http://www.semanticweb.org/ontologia-viral#SubFamilia
:SubFamilia rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf owl:topDataProperty ;
            rdfs:range xsd:string .


###  http://www.semanticweb.org/ontologia-viral#Tamanho
:Tamanho rdf:type owl:DatatypeProperty .


###  http://www.semanticweb.org/ontologia-viral#maiorTamanho
:maiorTamanho rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf :Tamanho ;
              rdfs:range xsd:decimal ,
                         xsd:integer ,
                         xsd:long .


###  http://www.semanticweb.org/ontologia-viral#menorTamanho
:menorTamanho rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf :Tamanho ;
              rdfs:range xsd:decimal ,
                         xsd:integer ,
                         xsd:long .


